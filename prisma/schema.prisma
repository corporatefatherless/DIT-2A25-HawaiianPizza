generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model carParts {
  id            Int     @id @default(autoincrement())
  name          String
  type          String
  price_display String
  description   String
  partId        String?
}

model cars {
  id            Int    @id @default(autoincrement())
  name          String
  type          String
  power         String
  price_display String
  description   String
  carsid        String
}
